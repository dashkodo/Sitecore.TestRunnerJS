<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--Add full path to the Sitecore.TestrunnerJS package folder-->
    <add key="Sitecore.TestRunnerJS.PackagePath" value="path_to_testrunner_package"/>
    
    <!--Add your website under test instance name-->
    <add key="Sitecore.TestRunnerJS.InstanceName" value="name_of_sitecore_instance"/>
    
    <!--Add your SPEAK application under test name-->
    <add key="Sitecore.TestRunnerJS.ApplicationName" value="name_of_speak_application"/>
  </appSettings>
</configuration>